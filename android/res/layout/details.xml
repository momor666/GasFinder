<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="fill_parent" android:layout_height="120px"
		android:orientation="horizontal">
		<ImageView android:id="@+id/ImageView01"
			android:layout_height="fill_parent" android:layout_width="120px"></ImageView>
		<LinearLayout android:id="@+id/LinearLayout03"
			android:orientation="vertical" android:layout_height="fill_parent"
			android:layout_width="fill_parent">
			<TextView android:layout_height="wrap_content" android:id="@+id/nome"
				android:text="nome" android:layout_width="fill_parent"></TextView>
			<TextView android:layout_height="wrap_content"
				android:layout_width="fill_parent" android:id="@+id/endereco"
				android:text="endereco"></TextView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout04"
		android:layout_height="170px" android:layout_width="fill_parent"></LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout05"
		android:layout_height="170px" android:layout_width="fill_parent"
		android:orientation="vertical">
		<LinearLayout android:id="@+id/LinearLayout07"
			android:layout_height="85px" android:layout_width="fill_parent"
			android:orientation="horizontal">
			<ImageView android:layout_height="85px"
				android:layout_width="85px" android:id="@+id/iconegasolina"></ImageView>
			<TextView android:layout_height="wrap_content" android:id="@+id/gasolina"
				android:text="preco gasolina" android:layout_width="135px"></TextView>
			<ImageView android:layout_height="85px"
				android:layout_width="85px" android:id="@+id/iconealcool"></ImageView>
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/alcool"
				android:text="preco alcool"></TextView>
		</LinearLayout>

<LinearLayout android:id="@+id/LinearLayout08" android:layout_height="fill_parent" android:layout_width="fill_parent" android:orientation="horizontal"><ImageView android:layout_height="85px" android:layout_width="85px" android:id="@+id/iconegnv"></ImageView>
<TextView android:layout_height="wrap_content" android:id="@+id/gnv" android:text="preco gnv" android:layout_width="135px"></TextView>
<ImageView android:layout_height="85px" android:layout_width="85px" android:id="@+id/iconediesel"></ImageView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/diesel" android:text="preco diesel"></TextView>
</LinearLayout>
</LinearLayout>
<LinearLayout android:id="@+id/LinearLayout06" android:layout_height="170px" android:layout_width="fill_parent"></LinearLayout>
</LinearLayout>
